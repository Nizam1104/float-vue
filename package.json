{
  "name": "float-vue",
  "author": "Nizam Vali",
  "email": "nizamvali1104@gmail.com",
  "homepage": "https://github.com/Nizam1104/float-vue",
  "license": "MIT",
  "version": "1.0.1",
  "description": "A Vue.js float component",
  "main": "dist/float-vue.umd.js",
  "module": "dist/float-vue.esm.js",
  "unpkg": "dist/float-vue.min.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/float-vue.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/float-vue.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/float-vue.min.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/compiler-sfc": "^3.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vite": "^5.4.3",
    "vue": "^3.2.0"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "vue-draggable-resizable": "^3.0.0"
  }
}
